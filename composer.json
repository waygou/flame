{
    "name": "waygou/flame",
    "description": "Flame - A Feature-driven development (FDD) Laravel package",
    "type": "library",
    "keywords": [
        "flame",
        "waygou",
        "laravel",
        "framework",
        "php"
    ],
    "require": {
