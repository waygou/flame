        "waygou/helpers": "*",
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\Flame\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waygou",
            "email": "bruno.falcao@waygou.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\Flame\\FlameServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
